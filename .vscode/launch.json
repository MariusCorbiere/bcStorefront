{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Firefox for BC",
			"type": "firefox",
			"request": "launch",
			"reAttach": true,
			"url": "http://localhost:4201",
			"webRoot": "${workspaceFolder}",
			"log": {
				"fileName": "${workspaceFolder}/.vscode/vscode-firefox-debug.log",
				"fileLevel": {
					"default": "Debug"
				},
				"consoleLevel": {
					"default": "Error"
				}
			}
		}
	]
}